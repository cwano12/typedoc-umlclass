{
    "name": "typedoc-umlclass",
    "version": "0.3.2",
    "description": "Plugin for TypeDoc that generates UML class diagrams",
    "author": {
        "name": "Kriszti√°n Balla",
        "url": "https://github.com/krisztianb"
    },
    "license": "ISC",
    "keywords": [
        "uml",
        "typedoc",
        "typedocplugin"
    ],
    "dependencies": {
        "plantuml-encoder": "^1.4.0",
        "plantuml-pipe": "^1.3.6",
        "progress": "^2.0.3",
        "queue-fifo": "^0.2.6"
    },
    "devDependencies": {
        "@types/node": "14.14.27",
        "@types/plantuml-encoder": "1.4.0",
        "@types/progress": "2.0.3",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "eslint": "7.26.0",
        "eslint-plugin-jsdoc": "34.2.2",
        "eslint-plugin-ordered-imports": "0.5.0",
        "eslint-plugin-unicorn": "32.0.1",
        "husky": "4.3.8",
        "prettier": "2.3.0",
        "rimraf": "3.0.2",
        "typedoc": "0.20.36",
        "typescript": "4.2.4"
    },
    "peerDependencies": {
        "typedoc": "^0.17.0 || ^0.18.0 || ^0.19.0 || ^0.20.14"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krisztianb/typedoc-umlclass.git"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run format && npm run lint"
        }
    },
    "main": "dist/index.js",
    "files": [
        "/dist"
    ],
    "scripts": {
        "lint": "npx eslint ./src",
        "format": "npx prettier --check ./src",
        "clean": "rimraf dist",
        "build": "npx tsc",
        "clean-build": "npm run clean && npm run build"
    }
}
